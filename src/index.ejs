  <section class="projects">
    <h2>Projects</h2>
    <ul>
          <% projects.projects.forEach((content) => { %>
            <li>
                <button data-project="<%= content.dataName %>">
                    <div>
                        <div>
                            <h3><%= content.name %></h3>
                            <% if (content.prize) { %>
                            <div tabindex="0">
                                <img src="images/trophy.png" aria-label="<%= content.prize %>" alt="An image of a trophy">
                                <p><%= content.prize %></p>
                            </div>
                            <% } %>
                        </div>
                        <p><%- content.projectDescription %></p>
                        <section>
                            <h4 class="visually-hidden">Skills used:</h4>
                            <ul>
                                <% content.usedSkills.forEach((skill) => { %>
                                    <li><%- skill %></li>
                                <% }); %>
                            </ul>
                        </section>
                        <section>
                            <h4 class="visually-hidden">More info:</h4>
                            <% if (content.linkToRepo) { %>
                                <a target="_blank" href="<%- content.linkToRepo %>">Github repo</a>
                            <% } %>
                            <% if (content.grade) { %>
                                <p>Grade: <span><%= content.grade %></span></p>
                            <% } %>
                            <p><span class="visually-hidden">Date: </span><%= content.date %></p>
                        </section>
                    </div>
                    <img
                        src="<%= content.img %>"
                        alt="<%= content.imgAlt %>"
                    />
                </button>
            </li>               
        <% }); %>
    </ul>
  </section>
           


